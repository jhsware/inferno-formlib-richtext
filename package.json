{
  "name": "inferno-formlib-richtext",
  "version": "4.0.0-beta.2",
  "description": "Medium style editing with inferno",
  "main": "index.js",
  "scripts": {
    "test": "echo \"TODO: Write tests...\"",
    "prepublish": "npm run build",
    "postversion": "git push && git push --tags",
    "build": "rm -rf ./dist && NODE_ENV=production babel lib --out-dir dist  --source-maps",
    "build-dev": "rm -rf ./dist && babel lib --out-dir dist --watch --source-maps",
    "build-test": "rm -rf ./test/dist && node_modules/.bin/webpack --config ./test/browser/webpack.config",
    "build-test-dev": "rm -rf ./test/dist && node_modules/.bin/webpack --watch --config ./test/browser/webpack.config"
  },
  "keywords": [
    "inferno",
    "isomorphic-schema",
    "formlib"
  ],
  "author": "Sebastian Ware <sebastian@urbantalk.se> (https://github.com/jhsware)",
  "license": "MIT",
  "peerDependencies": {
    "component-registry": "^0.2.8",
    "isomorphic-schema": "^0.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-inferno": "^4.0.0",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bootstrap": "^4.0.0",
    "cheerio": "^1.0.0-rc.2",
    "component-registry": "^0.2.8",
    "express": "^4.16.2",
    "inferno-devtools": "^4.0.8",
    "inferno-router": "^4.0.8",
    "isomorphic-schema": "^0.7.1",
    "webpack": "^2.7.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "inferno": "^4.0.8",
    "inferno-animation": "^4.0.0",
    "inferno-bootstrap": "^4.0.1",
    "inferno-formlib": "^4.0.1",
    "medium.js": "^1.0.1"
  }
}
